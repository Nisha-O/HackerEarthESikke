<h2>Transactions - <%= t_type %></h2>
  <p></p>            
  <table class="table table-striped">
    <thead>
     <tr>
        <th>Transaction id</th>
        <th>Transfered From</th>
        <th>Amount</th>
        <th>Date</th>
        <th>status</th>
    </tr>
    </thead>
    <tbody>
     <% transactions.each do |c_t| %>
    <tr id="tr_id_1" class="tr-class-1">
        <td><%= c_t.transaction_id %></td>
        <td id="td_id_1" class="td-class-1">
        <div><%= c_t.user.email %></div>
        <div><%= c_t.user.account_id %></div>
        </td>
        <td><%= c_t.amount.round.to_s %></td>
        <td><%= c_t.created_at.in_time_zone("Chennai").strftime("%I:%M %p on %d-%b-%y") %></td>
        <td><%= c_t.status %></td>
    </tr>
    <% end %>
    </tbody>
  </table>